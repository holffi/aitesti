{
    "html.autoClosingTags": true,
    "files.associations": {
        "*.ejs": "html"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[ejs]": {
        "html.autoClosingTags": false,
        "editor.formatOnSave": false,
        "editor.defaultFormatter": null
    }
}
